@using Nop.Core.Infrastructure;
@using Nop.Core.Domain.Customers
@using Telerik.Web.Mvc.UI;
@{
    var displayMiniProfiler = EngineContext.Current.Resolve<Nop.Core.Domain.StoreInformationSettings>().DisplayMiniProfilerInPublicStore;
    var _workContext = EngineContext.Current.Resolve<Nop.Core.IWorkContext>();

    Html.AppendScriptParts("~/Content/MiniSite/js/cusel-fixed-min-2.5.js");
    Html.AppendScriptParts("~/Content/MiniSite/js/validateRun.js");
    Html.AppendScriptParts("~/Content/MiniSite/js/jquery.validate.min.js");
    Html.AppendScriptParts("~/Content/MiniSite/js/jquery.diyslider.min.js");
    Html.AppendScriptParts("~/Content/MiniSite/js/script.js");
    Html.AppendScriptParts("~/Content/MiniSite/js/jquery-1.6.2.js");
    Html.AppendCssFileParts("~/Content/MiniSite/Templates/" + WorkContext.CurrentMiniSite.MiniSiteLayout.CssTemplate + "/css/theme.css");
    Html.AppendCssFileParts("~/Content/MiniSite/Templates/" + WorkContext.CurrentMiniSite.MiniSiteLayout.CssTemplate + "/css/sliderstyle" + WorkContext.CurrentMiniSite.LayoutId + ".css");
    Html.AppendCssFileParts("~/Content/MiniSite/Templates/" + WorkContext.CurrentMiniSite.MiniSiteLayout.CssTemplate + "/css/styles" + WorkContext.CurrentMiniSite.LayoutId + ".css");
    Html.AppendCssFileParts("~/Content/MiniSite/Templates/" + WorkContext.CurrentMiniSite.MiniSiteLayout.CssTemplate + "/css/cusel.css");
    Html.AppendCssFileParts("~/Content/MiniSite/Templates/" + WorkContext.CurrentMiniSite.MiniSiteLayout.CssTemplate + "/css/root.css");
}
<!DOCTYPE html>
<html @Html.Partial("LanguageAttributes")>
<head>
    <title>@Html.NopTitle(true)</title>
    <meta http-equiv="Content-type" content="text/html;charset=UTF-8" />
    <meta name="description" content="@(Html.NopMetaDescription())" />
    <meta name="keywords" content="@(Html.NopMetaKeywords())" />

    @*This is used so that themes can inject content into the header*@
    @Html.NopCssFiles(this.Url, ResourceLocation.Head)
    @Html.NopScripts(this.Url, ResourceLocation.Head,bundleFiles:false)
    @Html.NopCanonicalUrls()
    <script src="http://code.jquery.com/ui/1.10.2/jquery-ui.js"></script>
    @Html.Action("GoogleAnalytics","Common")
</head>
<body>
    @RenderBody()
    @Html.NopCssFiles(this.Url, ResourceLocation.Foot)
    @Html.NopScripts(this.Url, ResourceLocation.Foot)
</body>
</html>
